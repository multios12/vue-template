{
  "name": "vue-template",
  "description": "vue.js & Express & Bootstrap4を使用したWebページを作成するためのテンプレート",
  "version": "1.0.0",
  "scripts": {
    "start": "node dist/server.js",
    "build": "webpack --mode development && tsc",
    "release": "webpack --mode production && tsc",
    "watch": "run-p watch.*",
    "watch.ts": "tsc -w",
    "watch.bundle": "webpack --mode development --progress --watch"
  },
  "dependencies": {
    "axios": "0.18.0",
    "bootstrap-vue": "2.0.0-rc.20",
    "debug": "4.1.1",
    "express": "4.17.0",
    "http-errors": "1.7.2",
    "jquery": "3.4.1",
    "moment": "2.24.0",
    "vue": "2.6.10",
    "vue-router": "3.0.6"
  },
  "devDependencies": {
    "@types/express": "4.16.1",
    "@types/jquery": "3.3.29",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "npm-run-all": "4.1.5",
    "ts-loader": "6.0.0",
    "typescript": "3.4.5",
    "vue-loader": "15.7.0",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2"
  }
}
