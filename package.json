{
  "name": "vueTemplate",
  "description": "vue.js & Express & Bootstrap4を使用したWebページを作成するためのテンプレート",
  "main": "./dist/server.js",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/multios12/bootstrapTemplate.git"
  },
  "private": true,
  "scripts": {
    "all": "npm run start && npm run watch",
    "start": "node dist/server.js",
    "build": "node fuse build && tsc",
    "release": "node fuse release --production",
    "watch": "run-p watch.*",
    "watch.ts": "nodemon --ext ts --watch src -x  \"tsc\"",
    "watch.bundle": "nodemon --watch src/public -x  \"node fuse build\""
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.9",
    "@fortawesome/fontawesome-free-regular": "^5.0.9",
    "@fortawesome/fontawesome-free-solid": "^5.0.9",
    "@types/compression": "^0.0.36",
    "@types/datatables.net": "^1.10.9",
    "@types/express": "^4.11.1",
    "@types/jquery": "^3.3.1",
    "browser-sync": "^2.23.6",
    "connect-browser-sync": "^2.1.0",
    "fuse-box": "~3.2.2",
    "nodemon": "~1.17.2",
    "npm-run-all": "^4.1.2",
    "tar": "~4.4.1",
    "typescript": "^2.8.3",
    "uglify-es": "~3.3.9"
  },
  "dependencies": {
    "bootstrap": "~4.0.0",
    "compression": "^1.7.2",
    "cookie-parser": "~1.4.3",
    "datatables": "~1.10.13",
    "datatables.net-bs4": "~1.10.16",
    "debug": "~2.6.9",
    "ejs": "^2.5.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jquery": "~3.3.1",
    "moment": "~2.21.0",
    "morgan": "~1.9.0",
    "popper.js": "~1.14.1",
    "request": "~2.85.0",
    "request-promise": "~4.2.2",
    "vue": "^2.5.16"
  }
}
